#include <behaviors.dtsi>

#include "base.keymap"

/ {
  chosen {
    zmk,matrix_transform = &five_column_transform;
  };

  keymap {
    compatible = "zmk,keymap";
    default_layer {
      display-name = "QWERTY";
      bindings = <
        DTR
        DMR
        DBR(,)
        DTHR
      >;
    };

    gaming_layer {
      display-name = "GAMING";
      bindings = <
        XXXX XXXX XXXX XXXX XXXX  XXXX XXXX XXXX XXXX XXXX
        XXXX XXXX XXXX XXXX XXXX  XXXX XXXX XXXX XXXX XXXX
        XXXX XXXX XXXX XXXX XXXX  XXXX XXXX XXXX XXXX XXXX
                  XXXX XXXX XXXX  XXXX XXXX XXXX
      >;
    };
        
    num_nav_layer {
      display-name = "NAV";
      bindings = <
        NTR
        NMR
        NBR(,)
        NTHR
      >;
    };

    symbol_layer {
      display-name = "SYMBOL";
      bindings = <
        STR
        SMR
        SBR(,)
        STHR
      >;
    };
        
    adjust_layer {
      display-name = "ADJUST";
      bindings = <
        ATR
        AMR
        ABR(,)
        ATHR
      >;
    };
  };
};
